{
    "ruby.lint": {
        "reek": false,
        "rubocop": true,
        "ruby": false, //Runs ruby -wc
        "fasterer": true,
        "debride": false,
        "ruby-lint": false
    },
    "files.autoSave": "afterDelay",
    "[python]": {
        "editor.tabSize": 4
    },
    "[ruby]": {
        "editor.tabSize": 2
    },
    "[javascript]": {
        "editor.tabSize": 2
    },
    "python.formatting.autopep8Args": [
        "--max-line-length",
        "120",
    ]
}